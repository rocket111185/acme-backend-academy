{
    "name": "osf-backend-academy",
    "version": "0.0.1",
    "description": "OSF Backend Academy Project",
    "main": "index.js",
    "scripts": {
        "build": "npx rollup -c",
        "lint": "find . -path ./node_modules -prune -false -o -name '*.js' -o -name '*.json' | xargs -I{} npx prettier -w --check {} && npx eslint --fix .",
        "watch": "npx rollup -c -w",
        "server": "npx nodemon app/index.js --watch app/index.js --watch app/public/App.js",
        "start": "npx run-p watch server",
        "test": "npx jest"
    },
    "author": "Rekechynsky Dmytro",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.21.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "rollup": "^2.35.1",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "~7.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "sirv-cli": "^1.0.10",
        "svelte": "^3.31.1",
        "svelte-routing": "^1.6.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "eslint": "^7.30.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2"
    }
}
